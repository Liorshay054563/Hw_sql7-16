
8. ? מוחק בהטבלה את השורה שבה מופיע 'orrange' 
UPDATE shopping SET name = 'Bisli' WHERE name LIKE 'Bamba' – משנה את השם במבה לביסלי
UPDATE shopping SET amount=1 WHERE name LIKE 'Milk' – משנה את הכום של מילק ל1
9. ?
ALTER TABLE shopping ADD COLUMN maavar--- תוסיף לטבלה עוד עמודה בשם מעבר

10. ?
UPDATE shopping SET maavar=6 WHERE id=1; -- מוסיף את המספר 6 למעבר בשורה שבה נמצא ID 1
UPDATE shopping SET maavar=3 WHERE id=2; -
מוסיף את המספר 3 למעבר בשורה שבה נמצא ID 2
UPDATE shopping SET maavar=12 WHERE id=3; - 
מוסיף את המספר 12 למעבר בשורה שבה נמצא ID 3
UPDATE shopping SET maavar=8 WHERE id=4; --
מוסיף את המספר 8 למעבר בשורה שבה נמצא ID 4
UPDATE shopping SET maavar=5 WHERE id=5; --
11. ? מוסיף את המספר 6 למעבר בשורה שבה נמצא ID 1
SELECT * FROM shopping WHERE amount > 1 AND maavar > 5---
מציג את השורה שבה הסכום הוא גדול מ1 והמעבר גדול מ 5.
SELECT * FROM shopping WHERE maavar BETWEEN 3 AND 5
12. ? מציג את כל השורות שהערך של המעבר שלהם בין 3 ל 5
SELECT * FROM shopping ORDER BY maavar –
ממציג לי את הטבלה מסודרת לפי סדר המספרים של הערכים במעבר
SELECT * FROM shopping ORDER BY maavar DESC
13. ? מציג את הטבלה לפי הסדר של הערכים במעבר בסדר יורד
CREATE TABLE books (id INTEGER PRIMARY KEY, name TEXT);
INSERT INTO books VALUES (1, 'SQL PROGRAMMING');
INSERT INTO books VALUES (2, 'CSHARP PROGRAMMING');
DELETE FROM books;
14. ? יוצר טבלה חדשה בשם "books" עם רשומה בשם "id" as int וכמפתח ראשי ו"name" as str.
מוסיף את "sql" לשורה 1
ואת "csharp"  לשורה מס 2
SELECT COUNT(*)from shopping---
סופר כמה שורות יש בטבלה
SELECT MAX(amount) from shopping
מחזיר את הערך הגבוה ביותר בעמודה של "amount"
SELECT AVG(amount) from shopping
מחזיר את הממוצע בעמודה של "amount"
SELECT MIN(amount) from shopping
מחזיר את הערך הנמוך ביותר בעמודה של "amount"
15. ?
INSERT INTO shopping VALUES (6, 'Onions', 3, 6);
מוסיף לשורה 6 ,בצלים במעבר  3 והמחיר הוא 6
INSERT INTO shopping VALUES (7, 'Orio', 1, 8);
מוסיף לשורה 7 ,אוריאו במעבר  1 והמחיר הוא 8
Select maavar, COUNT(*)FROM shopping GROUP BY maavar
